{
  "name": "direction-sky",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=23.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "next build",
    "type-check": "tsc --noEmit",
    "start": "next start",
    "dev": "next dev",
    "lint": "next lint",
    "test": "jest",
    "deploy:all": "npm run deploy:functions && npm run deploy:scheduler",
    "deploy:functions": "gcloud functions deploy data-ingestion --gen2 --runtime=nodejs24 --region=us-central1 --source=src/functions --entry-point=dataIngestion --trigger-http --allow-unauthenticated --memory=512MB --timeout=540s",
    "deploy:scheduler": "gcloud scheduler jobs create http data-ingestion-scheduler --location=us-central1 --schedule='*/5 * * * *' --uri='https://us-central1-dev-epsilon-467101-v2.cloudfunctions.net/data-ingestion' --http-method=POST --message-body='{\"source\":\"scheduler\"}' --description='Data ingestion scheduler - every 5 minutes'",
    "deploy:polygon": "node scripts/deploy-polygon.js",
    "deploy:polygon-scheduler": "node scripts/setup-polygon-scheduler.js",
    "deploy:script": "node scripts/fetch-and-store-coinmarketcap.js",
    "logs": "gcloud functions logs read data-ingestion --limit=50",
    "logs:polygon": "gcloud functions logs read polygon-options-fetcher --limit=50",
    "test:polygon": "curl -X POST https://us-central1-dev-epsilon-467101-v2.cloudfunctions.net/polygon-health-check",
    "health:polygon": "curl https://us-central1-dev-epsilon-467101-v2.cloudfunctions.net/polygon-health-check",
    "test:composite-key": "node scripts/test-composite-primary-key.js",
    "test:upsert": "node scripts/test-upsert.js",
    "update:polygon-schema": "node scripts/update-polygon-schema.js"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.9.4",
    "@google-cloud/functions-framework": "^3.3.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@types/node": "^24.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.16",
    "axios": "^1.11.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^10.16.4",
    "ioredis": "^5.6.1",
    "next": "^13.4.19",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^13.4.19"
  }
}
